#include "pinSettings.h"

/*********************************************************************************************************************/
/*------------------------------------------------Function Prototypes------------------------------------------------*/
/*********************************************************************************************************************/
extern uint8 getSW(void);

/*********************************************************************************************************************/
/*------------------------------------------Inline Function Implementation-------------------------------------------*/
/*********************************************************************************************************************/
static inline void initSwitch(void)
{
    setPinInput(PIN_SW);  // 풀업모드 디지털 입력 핀으로 설정
}

#endif /* INCLUDES_HARDWARE_SWITCH_SWITCH_H_ */
